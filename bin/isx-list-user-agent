#!/bin/bash -eu
set -o pipefail

q=${1:-}

source "$(dirname "$(readlink -f "${BASH_SOURCE[0]}")")/x-lib.sh"
#-------------------------------------------------------------------------------
ask "org.href" "${ORG_HREF:-}"
org_href=$ASK_BUF
#-------------------------------------------------------------------------------
curla -f "$ENDPOINT_URL$org_href/user_agent?$q" | jq
#-------------------------------------------------------------------------------
