#!/bin/bash -eu
set -o pipefail

source "$(dirname "$(readlink -f "${BASH_SOURCE[0]}")")/x-lib.sh"
#-------------------------------------------------------------------------------
ask "plug_proc.href" "/plug_proc/$(uuidgen)"
echo "$ASK_BUF" > "$PLUG_PROC_HREF_"
reload_vars
#-------------------------------------------------------------------------------
"$(dirname "$(readlink -f "${BASH_SOURCE[0]}")")/isx-read" "$PLUG_PROC_HREF"
#-------------------------------------------------------------------------------
